<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Minecraft Message Board</title>
    <link rel="stylesheet" href="styles.css" />
  </head>
  <body>
    <main class="mainContainer">
      <div class="messages-container">
        <% messages.forEach(message => { %> <%- include('messageSign',
        {message:message}) %> <% }); %>
      </div>
    </main>
  </body>
</html>
